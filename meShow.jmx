            <stringProp name="script">import com.melot.kktv.util.SecurityFunctions;
import com.google.gson.JsonElement;
import com.google.gson.JsonObject;
import com.google.gson.JsonParser;
import com.alibaba.fastjson.JSONObject;
import java.net.URLEncoder;
import java.util.ArrayList;
//import java.util.Iterator;

String userId = vars.get(&quot;userId&quot;);
String password = vars.get(&quot;password&quot;);

String intputStr = &quot;u=&quot;+userId+&quot;&amp;p=&quot;+password;
String up = SecurityFunctions.upEncode(intputStr);
String json = vars.get(&quot;json&quot;);

JSONObject jo = JSONObject.parseObject(json);
jo.put(&quot;up&quot;,up);

JsonParser parser = new JsonParser();
JsonElement jsonEl = parser.parse(jo.toString()); 
JsonObject jsonObj = null;
jsonObj = jsonEl.getAsJsonObject();

String sv = SecurityFunctions.getSingedValue(jsonObj);
jo.put(&quot;sv&quot;,sv);

vars.put(&quot;json&quot;, URLEncoder.encode(jo.toJSONString(), &quot;UTF-8&quot;));
//print(vars.get(&quot;json&quot;));

</stringProp>
